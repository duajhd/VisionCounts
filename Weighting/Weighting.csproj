<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows10.0.19041.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="OIP.jpg" />
    <None Remove="Q.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="OIP.jpg" />
    <Resource Include="Q.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MaterialDesignColors" Version="5.2.1" />
    <PackageReference Include="MaterialDesignThemes" Version="5.2.1" />
    <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="9.0.3" />
    <PackageReference Include="QRCoder" Version="1.6.0" />
    <PackageReference Include="SkiaSharp" Version="3.116.1" />
    <PackageReference Include="System.Data.SQLite" Version="1.0.119" />
    <PackageReference Include="System.IO.Ports" Version="9.0.4" />
    <PackageReference Include="Zebra.Printer.SDK" Version="3.0.3337" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Kingdee.BOS.WebApi.Client">
      <HintPath>bin\Debug\net8.0-windows10.0.19041.0\Kingdee.BOS.WebApi.Client.dll</HintPath>
    </Reference>
    <Reference Include="Kingdee.CDP.WebApi.SDK">
      <HintPath>bin\Debug\net8.0-windows10.0.19041.0\Kingdee.CDP.WebApi.SDK.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
